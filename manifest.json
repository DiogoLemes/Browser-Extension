{
  "manifest_version": 3,
  "name": "Remove Youtube AI Summary",
  "version": "1.0",
  "description": "Removes the AI summaries from YouTube video pages.",
  
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],

  "permissions": []
}
